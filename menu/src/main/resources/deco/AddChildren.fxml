<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="BOTTOM_CENTER" spacing="20.0" HBox.hgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER_LEFT" fillHeight="false" styleClass="crud-textfield" stylesheets="@deco.css">
               <children>
                  <Label stylesheets="@deco.css" text="CHILDREN NAME">
                     <styleClass>
                        <String fx:value="deco-font" />
                        <String fx:value="deco-font-bold" />
                        <String fx:value="font-size-s" />
                     </styleClass>
                  </Label>
                  <TextField promptText="CHILDREN NAME" stylesheets="@deco.css" HBox.hgrow="ALWAYS">
                     <styleClass>
                        <String fx:value="deco-font" />
                        <String fx:value="transparent-textfield" />
                        <String fx:value="font-size-s" />
                     </styleClass>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" fillHeight="false" styleClass="crud-textfield" stylesheets="@deco.css">
               <children>
                  <Label stylesheets="@deco.css" text="PLACE OF BIRTH">
                     <styleClass>
                        <String fx:value="deco-font" />
                        <String fx:value="deco-font-bold" />
                        <String fx:value="font-size-s" />
                     </styleClass>
                  </Label>
                  <TextField promptText="PLACE OF BIRTH" stylesheets="@deco.css" HBox.hgrow="ALWAYS">
                     <styleClass>
                        <String fx:value="deco-font" />
                        <String fx:value="transparent-textfield" />
                        <String fx:value="font-size-s" />
                     </styleClass>
                  </TextField>
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox spacing="10.0">
         <children>
            <VBox>
               <children>
                  <Label stylesheets="@deco.css" text="DATE OF BIRTH">
                     <styleClass>
                        <String fx:value="deco-font" />
                        <String fx:value="deco-font-bold" />
                        <String fx:value="font-size-s" />
                     </styleClass>
                     <VBox.margin>
                        <Insets left="20.0" />
                     </VBox.margin>
                  </Label>
                  <DatePicker maxWidth="1.7976931348623157E308" promptText="DATE OF BIRTH" stylesheets="@deco.css">
                     <styleClass>
                        <String fx:value="crud-padding-datePicker" />
                        <String fx:value="deco-font" />
                        <String fx:value="deco-font-bold" />
                        <String fx:value="font-size-s" />
                     </styleClass>
                  </DatePicker>
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label stylesheets="@deco.css" text="GENDER">
                     <styleClass>
                        <String fx:value="deco-font" />
                        <String fx:value="deco-font-bold" />
                        <String fx:value="font-size-s" />
                     </styleClass>
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                  </Label>
                  <ComboBox maxWidth="1.7976931348623157E308" promptText="GENDER" stylesheets="@deco.css">
                     <styleClass>
                        <String fx:value="default-button" />
                        <String fx:value="font-size-s" />
                        <String fx:value="deco-font-bold" />
                     </styleClass>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </ComboBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</HBox>
